{
  "expo": {
    "name": "SiteSnap",
    "slug": "site-snap-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "updates": {
      "enabled": false
    },
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a5fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sitesnap.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a5fff"
      },
      "package": "com.sitesnap.app"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload product images."
        }
      ],
      [
        "expo-document-picker"
      ]
    ]
  }
}
